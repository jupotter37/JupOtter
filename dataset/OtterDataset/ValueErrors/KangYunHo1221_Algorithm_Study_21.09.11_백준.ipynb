    "## LIS(증가하는 부분수열)\n",
    "import sys\n",
    "\n",
    "n = int(input())\n",
    "number = list(map(int, sys.stdin.readline().split()))\n",
    "\n",
    "LIS = [1]*n\n",
    "\n",
    "for i in range(1, n):\n",
    "    for j in range(i):\n",
    "        if number[i] > number[j]:\n",
    "            LIS[i] = max(LIS[i], LIS[j]+1)\n",
    "\n",
    "print(max(LIS))"
