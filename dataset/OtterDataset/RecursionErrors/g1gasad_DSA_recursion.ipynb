   "source": [
    "def rec_fact(n):\n",
    "    if n <= 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return n * rec_fact(n-1)\n",
    "    \n",
    "def rec_sum(n):\n",
    "    if n == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return n + rec_sum(n-1)\n",
    "    \n",
    "# rec_fact(4)\n",
    "rec_sum(10)"
