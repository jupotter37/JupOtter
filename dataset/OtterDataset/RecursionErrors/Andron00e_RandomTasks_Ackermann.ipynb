      "source": [
        "def ackermann (n, m):\n",
        "  if n == 0:\n",
        "    return m + 1\n",
        "  elif m == 0: \n",
        "    return ackermann (n - 1, 1)\n",
        "  else: \n",
        "    return ackermann (n - 1, ackermann (n, m - 1))\n"
      ]
    },
